<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.cyanrocks.ai.dao.mapper.AiGbiTableMapper">
    <resultMap id="AiGbiTable" type="com.cyanrocks.ai.dao.entity.AiGbiTable">
        <id property="id" column="id"/>
        <result property="milvusId" column="milvus_id"/>
        <result property="tableName" column="table_name"/>
        <result property="field" column="field"/>
        <result property="searchSql" column="search_sql"/>
        <result property="metedate" column="metedate"/>
    </resultMap>
    <select id="getGbiTablePage" resultMap="AiGbiTable">
        select * from ai_gbi_table
        <if test="search != '' and search != null">
            where ${search}
        </if>
        order by
        <if test="sort != '' and sort != null">
            ${sort},
        </if>
        id desc
    </select>

</mapper>
